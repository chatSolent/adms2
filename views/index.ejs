<h1>Student Data</h1>

<% if (typeof students !== 'undefined' && students.length> 0) { %>
<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        
      </tr>
    </thead>
    <tbody>
        <% students.forEach( function(student, index){ %>
      <tr>
        <th scope="row"><%- index+1 %></th>
        <td><%- student.name %></td>
        <td><%- student.email %></td>
        
      </tr>
      <% })  %>
    </tbody>
  </table>
  <% } %>


