<h1>Student Data</h1>

<% if (typeof students.students !== 'undefined' && students.students.length> 0) { %>
<table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Actions</th>


      </tr>
    </thead>
    <tbody>
        <% students.students.forEach( function(student, index){ %>
      <tr>
        <th scope="row"><%- index+1 %></th>
        <td><%- student.name %></td>
        <td><%- student.email %></td>
        <td><a href="../edit_student/<%- student._id %>">Update</a>| <a href="../delete_student/<%- student._id %>">Delete</a></td>


      </tr>
      <% })  %>
    </tbody>
  </table>
  <% } %>